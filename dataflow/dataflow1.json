{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Blobdata",
						"type": "DatasetReference"
					},
					"name": "Blobdata"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "Sink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          RowID as integer,",
				"          OrderPriority as integer,",
				"          Discount as float,",
				"          UnitPrice as float,",
				"          ShippingCost as float,",
				"          CustomerID as integer,",
				"          CustomerName as string,",
				"          ShipMode as string,",
				"          CustomerSegment as string,",
				"          ProductCategory as string,",
				"          ProductSubCategory as string,",
				"          ProductContainer as string,",
				"          ProductName as string,",
				"          ProductBaseMargin as float,",
				"          Region as string,",
				"          StateorProvince as string,",
				"          City as string,",
				"          PostalCode as integer,",
				"          OrderDate as date,",
				"          ShipDate as date,",
				"          Profit as float,",
				"          Quantityorderednew as integer,",
				"          Sales as float,",
				"          OrderID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> Blobdata",
				"Blobdata sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          RowID as integer,",
				"          OrderPriority as integer,",
				"          Discount as double,",
				"          UnitPrice as double,",
				"          ShippingCost as double,",
				"          CustomerID as integer,",
				"          CustomerName as string,",
				"          ShipMode as string,",
				"          CustomerSegment as string,",
				"          ProductCategory as string,",
				"          ProductSubCategory as string,",
				"          ProductContainer as string,",
				"          ProductName as string,",
				"          ProductBaseMargin as double,",
				"          Region as string,",
				"          StateorProvince as string,",
				"          City as string,",
				"          PostalCode as integer,",
				"          OrderDate as date,",
				"          ShipDate as date,",
				"          Profit as double,",
				"          Quantityorderednew as integer,",
				"          Sales as double,",
				"          OrderID as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> Sink"
			]
		}
	}
}