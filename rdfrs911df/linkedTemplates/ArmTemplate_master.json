{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rdfrs911df"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"GlobalSuperStoreData_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'GlobalSuperStoreData'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sdbkv.vault.azure.net/"
		},
		"PEP-DSDL-EA-SOUVIK-01_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/56ab1c0d-a1b3-49f1-8a28-5e1fac471eb7/resourceGroups/rgdb/providers/Microsoft.Sql/servers/sql-rg-dev-01"
		},
		"PEP-DSDL-EA-SOUVIK-01_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rdfrs911df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"GlobalSuperStoreData_connectionString": {
						"value": "[parameters('GlobalSuperStoreData_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"PEP-DSDL-EA-SOUVIK-01_properties_privateLinkResourceId": {
						"value": "[parameters('PEP-DSDL-EA-SOUVIK-01_properties_privateLinkResourceId')]"
					},
					"PEP-DSDL-EA-SOUVIK-01_properties_groupId": {
						"value": "[parameters('PEP-DSDL-EA-SOUVIK-01_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "rdfrs911df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'rdfrs911df_ArmTemplate_0')]"
			]
		}
	]
}